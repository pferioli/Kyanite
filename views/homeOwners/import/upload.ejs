<%- include ("../../partials/header") %>

<main class="site-content">

    <div class="container" style="margin-top: 50px;">
        <form class="col s12" method="post" action='/homeOwners/import/new/<%=data.client.id%>' enctype="multipart/form-data">
            <div class="card">
                <div class="card-content">
                    <span class="card-title">Importaci√≥n de propietarios</span>
                    <div class="row" style="margin-top: 20px;">
                        <div class="input-field col s9">
                            <i class="material-icons prefix">location_city</i>
                            <input id="client" name="client" type="text" value="<%=data.client.name%> [<%=data.client.internalCode%>]" disabled />
                            <label for="name">Nombre del Barrio</label>
                        </div>
                        <div class="input-field col s3">
                            <i class="material-icons prefix">label</i>
                            <input id="codigo" value="<%=data.client.internalCode%>" type="text" disabled/>
                            <label for="codigo">Codigo Interno</label>
                    </div>
                    </div>

                    <div class="row">
                        <div class="file-field input-field col s12">
                            <div class="btn"><i class="material-icons center">cloud_upload</i>
                                <input type="file" name="attachment" id="attachment">
                            </div>
                            <div class="file-path-wrapper">
                                <input class="file-path validate" type="text">
                                <label>seleccione el archivo de importacion de propietarios</label>
                            </div>
                        </div>                       
                    </div>

                </div>

                <div class="card-action right-align">
                    
                    <button id="button-submit" type='submit' class="waves-effect waves-light btn green" style="margin-right: 10px;">
                        <i class="material-icons right">save</i>Guardar</button>
                   
                    <button action="action" onclick="window.history.go(-1); return false;" type="submit" value="Cancel" class="waves-effect waves-light btn red"><i class="material-icons right">cancel</i>Volver</button>
                </div>
            </div>
            
            <input type="hidden" name="clientId" id="clientId" value=<%=data.client.id%> />

        </form>
    </div>
    <center>
        <%- include ("../../partials/alerts") %>
    </center>
</main>

<%- include ("../../partials/footer") %>
